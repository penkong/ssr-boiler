{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "engines": {
    "node": "11.7.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production",
    "dev-server": "webpack-dev-server",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "clean": "rimraf dist",
    "build": "set NODE_ENV=production npm run clean && webpack -p",
    "serve": "webpack-dev-server",
    "deploy": "npm run build && git subtree push --prefix dist gh-pages",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "webhook": "./sendgrid_webhook.sh",
    "dev-ssr": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penkong/landing-page.git"
  },
  "author": "Mazdak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/penkong/landing-page/issues"
  },
  "homepage": "https://github.com/penkong/landing-page#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.17.0",
    "express-http-proxy": "^1.5.1",
    "history": "^4.9.0",
    "lodash": "^4.17.21",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "path-parser": "^4.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-modal": "^3.8.1",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "serialize-javascript": "^1.7.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "image-webpack-loader": "^4.6.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}